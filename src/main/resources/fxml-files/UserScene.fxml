            <children>
                <Button fx:id="onEditUserCredentialsClick" layoutX="12.0" layoutY="656.0" mnemonicParsing="false"
                        onMouseClicked="#onEditUserCredentialsClick" prefHeight="39.0" prefWidth="242.0"
                        style="-fx-background-color: #7775;" text="Edytuj dane" textFill="WHITE">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Button>
                <Label fx:id="helloUser" layoutX="12.0" layoutY="14.0" prefHeight="43.0" prefWidth="242.0"
                       text="Witaj, Imię" textFill="#b5b5b5">
                    <font>
                        <Font size="26.0"/>
                    </font>
                </Label>
                <Label layoutX="12.0" layoutY="65.0" prefHeight="36.0" prefWidth="242.0" text="Twoje dane:"
                       textFill="#b5b5b5">
                    <font>
                        <Font size="20.0"/>
                    </font>
                </Label>
                <VBox layoutX="12.0" layoutY="136.0" spacing="5.0">
                    <children>
                        <Label fx:id="labelName" prefHeight="36.0" prefWidth="242.0" text="Imię: " textFill="#b5b5b5">
                            <font>
                                <Font size="17.0"/>
                            </font>
                        </Label>
                        <Label fx:id="labelSurname" prefHeight="36.0" prefWidth="242.0" text="Nazwisko: "
                               textFill="#b5b5b5">
                            <font>
                                <Font size="17.0"/>
                            </font>
                        </Label>
                        <Label fx:id="labelAge" prefHeight="36.0" prefWidth="242.0" text="Wiek: " textFill="#b5b5b5">
                            <font>
                                <Font size="17.0"/>
                            </font>
                        </Label>
                        <Label fx:id="labelAddress" prefHeight="36.0" prefWidth="242.0" text="Adres: "
                               textFill="#b5b5b5">
                            <font>
                                <Font size="17.0"/>
                            </font>
                        </Label>
                        <Label fx:id="labelZipcode" prefHeight="36.0" prefWidth="242.0" text="Kod pocztowy: "
                               textFill="#b5b5b5">
                            <font>
                                <Font size="17.0"/>
                            </font>
                        </Label>
                        <Label fx:id="labelCity" prefHeight="36.0" prefWidth="242.0" text="Miasto: " textFill="#b5b5b5">
                            <font>
                                <Font size="17.0"/>
                            </font>
                        </Label>
                        <Label fx:id="labelPhoneNumber" prefHeight="36.0" prefWidth="242.0" text="Telefon: "
                               textFill="#b5b5b5">
                            <font>
                                <Font size="17.0"/>
                            </font>
                        </Label>
                        <Label fx:id="labelEmail" prefHeight="36.0" prefWidth="242.0" text="Email: " textFill="#b5b5b5">
                            <font>
                                <Font size="17.0"/>
                            </font>
                        </Label>
                    </children>
                </VBox>
            </children>
        </AnchorPane>
        <Label fx:id="clockLabel" layoutX="241.0" layoutY="-1.0" prefHeight="53.0" prefWidth="205.0" textFill="WHITE">
            <font>
                <Font size="36.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
