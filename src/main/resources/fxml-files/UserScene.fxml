<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="768.0" prefWidth="1024.0" style="-fx-background-color: #CACACA;" styleClass="bodybg"
            stylesheets="@../css-files/User_background.css" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.UserController">
    <children>
        <Label layoutY="-2.0" prefHeight="53.0" prefWidth="1028.0" style="-fx-background-color: #525252;"
               text=" Widok klienta" textFill="WHITE">
            <font>
                <Font size="36.0"/>
            </font>
        </Label>
        <Button layoutX="864.0" mnemonicParsing="false" onMouseClicked="#onLogoutClick" prefHeight="45.0"
                prefWidth="164.0" style="-fx-background-color: #525252;" text="Wyloguj się" textFill="WHITE">
            <font>
                <Font size="23.0"/>
            </font>
        </Button>
        <AnchorPane layoutX="235.0" layoutY="51.0" prefHeight="718.0" prefWidth="793.0">
            <children>
                <VBox layoutX="73.0" layoutY="55.0" spacing="40.0">
                    <children>
                        <Button mnemonicParsing="false" onAction="#onViewToursClick" prefHeight="36.0" prefWidth="647.0"
                                style="-fx-background-color: #353238; -fx-opacity: 90%;"
                                text="Zapoznaj się z ofertą wycieczek" textFill="WHITE" wrapText="true">
                            <font>
                                <Font size="42.0"/>
                            </font>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#onManageReservationClick" prefHeight="93.0"
                                prefWidth="647.0" style="-fx-background-color: #353238; -fx-opacity: 90%;"
                                text="Zarządzaj rezerwacjami" textFill="WHITE" wrapText="true">
                            <font>
                                <Font size="42.0"/>
                            </font>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#onViewCompanyContactClick" prefHeight="93.0"
                                prefWidth="647.0" style="-fx-background-color: #353238; -fx-opacity: 90%;"
                                text="Kontakt z infolinią" textFill="WHITE" wrapText="true">
                            <font>
                                <Font size="42.0"/>
                            </font>
                        </Button>
                    </children>
                </VBox>
                <Line endX="-493.0" endY="63.0" layoutX="266.0" layoutY="56.0" startX="-241.0" startY="63.0"
